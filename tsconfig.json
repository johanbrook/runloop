{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "moduleResolution": "Node",
        "module": "ES6",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src"]
}
